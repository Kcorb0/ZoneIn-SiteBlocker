{
    "name": "ZoneIn",
    "version": "0.1.1",
    "description": "A site blocking extension to zone in on what is important",
    "permissions": [
        "storage",
        "tabs"
    ],
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "ZoneIn",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/eye_ripple.png",
            "32": "images/eye_ripple.png",
            "48": "images/eye_ripple.png",
            "128": "images/eye_ripple.png"
        }
    },
    "icons": {
        "16": "images/eye_ripple.png",
        "32": "images/eye_ripple.png",
        "48": "images/eye_ripple.png",
        "128": "images/eye_ripple.png"
    }
}